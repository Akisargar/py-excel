{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "select2/select2",
                "version": "4.0.13",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/select2/select2.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "kenwheeler/slick",
                "version": "1.8.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/kenwheeler/slick.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "acquia/acquia-cms-starterkit": "^1.1",
        "composer/installers": "^2.0",
        "drupal/acquia_analytics_redirects": "^1.0",
        "drupal/acquia_claro": "^1.3",
        "drupal/acquia_cms_dam": "^1.1",
        "drupal/acquia_cms_image": "^1.6",
        "drupal/acquia_cms_page": "^1.6",
        "drupal/acquia_cms_search": "^1.6",
        "drupal/acquia_cms_site_studio": "^1.6",
        "drupal/acquia_cms_toolbar": "^1.4",
        "drupal/acquia_cms_tour": "^2.1",
        "drupal/admin_toolbar": "^3.5",
        "drupal/advagg": "^6.0@alpha",
        "drupal/akamai": "^5.1",
        "drupal/autologout": "^1.4",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/captcha": "^2.0",
        "drupal/checklistapi": "^2.1",
        "drupal/clientside_validation": "^4.1",
        "drupal/commerce": "^2.36",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/content_language_access": "^2.0@beta",
        "drupal/core-composer-scaffold": "10.3.10",
        "drupal/core-project-message": "10.3.10",
        "drupal/core-recommended": "10.3.10",
        "drupal/crop": "^2.4",
        "drupal/csv_serialization": "^4.0",
        "drupal/devel_entity_updates": "^4.1",
        "drupal/devel_php": "^1.6",
        "drupal/disable_field": "^3.0",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/eu_cookie_compliance": "^1.24",
        "drupal/focal_point": "^2.0@alpha",
        "drupal/gdpr": "^3.1",
        "drupal/gdpr_fields": "^3.0",
        "drupal/geofield": "^1.62",
        "drupal/geolocation": "^3.12",
        "drupal/google_analytics": "^4.0",
        "drupal/google_tag": "^2.0",
        "drupal/hidden_language": "^2.0@beta",
        "drupal/honeypot": "^2.2",
        "drupal/hreflang": "^1.14",
        "drupal/http_cache_control": "^2.1",
        "drupal/hubspot": "^3.0@alpha",
        "drupal/lang_dropdown": "^2.1",
        "drupal/language_cookie": "^2.0",
        "drupal/loqate": "^3.0",
        "drupal/mailsystem": "^4.4",
        "drupal/mass_pwreset": "^2.1@beta",
        "drupal/media_library_edit": "^3.0",
        "drupal/menu_breadcrumb": "^2.0@alpha",
        "drupal/menu_item_extras": "^3.0",
        "drupal/menu_link_attributes": "^1.3",
        "drupal/migrate_plus": "^6.0",
        "drupal/migrate_source_csv": "^3.5",
        "drupal/migrate_tools": "^6.0",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/moderated_content_bulk_publish": "^2.0",
        "drupal/paragraphs": "^1.18",
        "drupal/path_redirect_import": "^2.0",
        "drupal/phpmailer_smtp": "^2.2",
        "drupal/raven": "^5.0",
        "drupal/recaptcha": "^3.4",
        "drupal/reroute_email": "^2.2",
        "drupal/search_api": "^1.37",
        "drupal/security_review": "^2.0",
        "drupal/selective_better_exposed_filters": "^3.0",
        "drupal/seo_checklist": "^5.2",
        "drupal/shield": "^1.8",
        "drupal/simple_recaptcha": "^1.0@beta",
        "drupal/smart_ip": "^5.0@alpha",
        "drupal/svg_image": "^3.1",
        "drupal/tmgmt_lilt": "^1.1",
        "drupal/token_filter": "^2.2",
        "drupal/translatable_menu_link_uri": "^2.1",
        "drupal/views_bulk_operations": "^4.3",
        "drupal/views_conditional": "^1.10",
        "drupal/views_data_export": "^1.4",
        "drupal/views_load_more": "^2.0",
        "drupal/webform_views": "^5.5",
        "drupal/yoast_analysis": "^1.1",
        "drush/drush": "^12.5",
        "kenwheeler/slick": "^1.8",
        "mnsami/composer-custom-directory-installer": "^2.0",
        "psr/http-message": "^1.1",
        "select2/select2": "^4.0",
        "wikimedia/composer-merge-plugin": "^2.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp-shim": true,
            "mnsami/composer-custom-directory-installer": true,
            "cweagans/composer-patches": true,
            "wikimedia/composer-merge-plugin": true,
            "axelerant/drupal-quality-checker": true
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            },
            "file-mapping": {
                "[web-root]/robots.txt": {
                    "mode": "replace",
                    "path": "docroot/core/assets/scaffold/files/robots.txt",
                    "overwrite": false
                },
                "[web-root]/.htaccess": {
                    "mode": "replace",
                    "path": "docroot/core/assets/scaffold/files/htaccess",
                    "overwrite": false
                }
            }
        },
        "installer-paths": {
            "docroot/core": ["type:drupal-core"],
            "docroot/libraries/{$name}": ["type:drupal-library", "nnnick/chartjs"],
            "docroot/modules/contrib/{$name}": ["type:drupal-module"],
            "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
            "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "docroot/modules/custom/{$name}": ["type:drupal-custom-module"],
            "docroot/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "docroot/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "enable-patching": "true",
        "patches": {
            "drupal/smart_ip": {
                "TypeError: Cannot assign string to property": "https://www.drupal.org/files/issues/2023-06-19/smart_ip-cannot-assign-latitude-3350345-5.patch"
            },
            "hubspot/hubspot-php": {
                "PHP 8.1": "https://patch-diff.githubusercontent.com/raw/HubSpot/hubspot-php/pull/378.patch"
            },
            "drupal/hubspot": {
                "PHP 8.1 Declaration issue": "https://www.drupal.org/files/issues/2023-09-19/hubspot-lock-psr-http-message-3388150-2.patch",
                "Fix Hubspot mapping": "patches/fix-hubspot-mapping.patch"
            },
            "drupal/views_data_export": {
                "drush command support": "https://www.drupal.org/files/issues/2023-09-28/2887450-89.views_data_export.Support-views-data-export-using-drush_0.patch",
                "Update export path": "patches/views_data_export.patch"
            },
            "drupal/acquia_cms_dam": { 
                "Update acquia_dam version": "patches/acquia_cms_dam_update_version_fix.patch"
            },
            "drupal/acquia_dam": {
                "Queue processing issue because of Exception": "patches/acquia_dam_update_queue_fixer.patch"
            },
            "drupal/geolocation": {
                "Lat/Lng Widget fails to save when no float given": "https://www.drupal.org/files/issues/2022-01-11/3257751-5.patch"
            },
            "acquia/cohesion": {
                "Add custom product variation linking functionality": "patches/custom_product_variation_linking.patch",
                "Fixed css render issue": "patches/acquia-claro-css.patch"
            },
            "drupal/commerce": {
                "Ability to unpublish a product varation": "https://drupal.org/files/issues/2023-11-28/3137498-17.patch"
            },
            "drupal/google_analytics": {
                "Patch for adding different GA ID with reseptect to language": "patches/google_analytics_language_implementation.patch"
            },
            "drupal/simple_sitemap": {
                "Patch for adding X-Default": "patches/provide-x-default.patch"
            },
            "drupal/loqate": {
                "Changes in the address fields": "patches/address_field_changes_patch.patch"
            },
            "drupal/core": {
                "Can't translate config when source language does not exists": "https://www.drupal.org/files/issues/2023-02-06/3184569_14.patch",
                "Passing null to parameter #1 ($data) of type string is deprecated": "patches/passing_null_deprecation.patch",
                "Fixed CKEditor content edit consistency": "https://www.drupal.org/files/issues/2024-06-05/3443422.patch",
                "ck-balloon-panel not visible for CKE5 instances in a Drupal dialog": "https://www.drupal.org/files/issues/2024-07-02/3351603-30.patch"
            },
            "drupal/views_load_more": {
                "The result summary is not refreshing after loading more results for the Ajax view": "https://www.drupal.org/files/issues/2023-12-11/views_load_more-3407928-1.patch"
            }
      },
        "merge-plugin": {
            "include": [
                "docroot/modules/contrib/webform/composer.libraries.json"
            ]
        }
    },
    "require-dev": {
        "axelerant/drupal-quality-checker": "^1.3",
        "mglaman/phpstan-drupal": "^1.2",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1"
    }
}
